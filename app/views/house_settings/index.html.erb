<p id="notice"><%= notice %></p>

<h1>Listing House Settings</h1>

<table>
  <thead>
    <tr>
      <th>House</th>
      <th>Setting name</th>
      <th>Setting value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @house_settings.each do |house_setting| %>
      <tr>
        <td><%= house_setting.house_id %></td>
        <td><%= house_setting.setting_name %></td>
        <td><%= house_setting.setting_value %></td>
        <td><%= link_to 'Show', house_setting %></td>
        <td><%= link_to 'Edit', edit_house_setting_path(house_setting) %></td>
        <td><%= link_to 'Destroy', house_setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New House setting', new_house_setting_path %>
