<p id="notice"><%= notice %></p>

<h1>Listing Recurring Frequencies</h1>

<table>
  <thead>
    <tr>
      <th>Frequency</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @recurring_frequencies.each do |recurring_frequency| %>
      <tr>
        <td><%= recurring_frequency.frequency %></td>
        <td><%= recurring_frequency.description %></td>
        <td><%= link_to 'Show', recurring_frequency %></td>
        <td><%= link_to 'Edit', edit_recurring_frequency_path(recurring_frequency) %></td>
        <td><%= link_to 'Destroy', recurring_frequency, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Recurring frequency', new_recurring_frequency_path %>
